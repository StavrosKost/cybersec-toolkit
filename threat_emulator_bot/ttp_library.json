      "id": "T1059.001",
      "name": "PowerShell Execution Example",
      "description": "Uses PowerShell to list top 5 processes by CPU.",
      "platform": "windows",
      "command": "powershell.exe -Command \"Get-Process | Sort-Object CPU -Descending | Select-Object -First 5\"",
      "tactic": "execution"
    },
    {
